<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="akka" type="Akka.Configuration.Hocon.AkkaConfigurationSection, Akka" />
  </configSections>
<appSettings>
    <add key="LocalID" value="1"/>
    <add key="LocalActorSystemName" value="LocalSystem"/>
    <add key ="RetryServerConnectTime" value="3000"/>
    <add key="ServerActorSystemName" value ="RCCServer"/>
    <add key="ServerIP" value="192.168.100.109"/>
    <add key="ServerPort" value="8081"/>
  </appSettings>
  <akka>
    <hocon>
      <![CDATA[
          akka {
            actor {
              provider = "Akka.Remote.RemoteActorRefProvider, Akka.Remote" 
            }
            remote {
              dot-netty.tcp {
                  port = 0
                  hostname = "localhost"
              }
            }
          }  
          
      ]]>
    </hocon>
  </akka>  
  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup> 
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Diagnostics.DiagnosticSource" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.1.0" newVersion="4.0.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.IO.Compression" publicKeyToken="b77a5c561934e089" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.1.2.0" newVersion="4.1.2.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>